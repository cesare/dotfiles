[user]
  name = SAWADA Tadashi
  email = cesare@mayverse.jp

[core]
  precomposeunicode = true
  quotepath = false

[alias]
  b   = branch
  ci  = commit
  co  = checkout
  dfc = diff --cached
  dfn = diff --name-only
  st  = status
  rpo = remote prune origin

[color]
  branch = auto
  diff = auto
  status = auto
  ui = always

[ghq]
  root = ~/git

[push]
  default = simple
